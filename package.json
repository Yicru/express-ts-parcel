{
  "name": "express-ts-parcel",
  "version": "1.0.0",
  "description": "Express TypeScript Parcel",
  "license": "MIT",
  "scripts": {
    "dev": "parcel --target node src/server.ts",
    "build": "parcel build --target node src/server.ts",
    "start": "node dist/server.js",
    "format": "prettier --write './src/**/*.{ts,tsx}'",
    "type": "tsc --noEmit",
    "type:watch": "tsc --noEmit --watch",
    "eslint": "eslint -c ./.eslintrc.json 'src/**/*.{ts,tsx}'",
    "clean": "rm -rf .cache dist"
  },
  "dependencies": {
    "express": "^4.17.3"
  },
  "devDependencies": {
    "@types/express": "^4.17.0",
    "eslint-config-prettier": "^6.0.0",
    "eslint-plugin-prettier": "^3.1.0",
    "parcel-bundler": "^1.12.3",
    "prettier": "^1.18.2",
    "typescript": "^3.5.2"
  }
}
